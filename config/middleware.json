{
    "timeout": 100,
    "load": {
        "before": [
            "responseTime",
            "logger",
            "cors",
            "responses",
            "gzip"
        ],
        "order": [
            "parser",
            "validateArticulo"
        ],
        "after": [
            "router"
        ]
    },
    "settings": {
        "validateArticulo": {
            "enabled": true
        },
        "parser": {
            "enabled": true,
            "multipart": true,
            "formLimit": "500mb",
            "jsonLimit": "200mb",
            "formidable": {
                "maxFileSize": 524288000
            }
        }
    }
}