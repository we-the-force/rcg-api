        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "source_url": {
      "type": "string"
    },
    "Radio_TV": {
      "type": "boolean",
      "default": false
    },
    "url": {
      "type": "uid",
      "targetField": "nombre",
      "required": true
    },
    "descripcion": {
      "type": "text"
    }
  }
}
