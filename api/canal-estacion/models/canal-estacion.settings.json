{
  "kind": "collectionType",
  "collectionName": "canal_estacions",
  "info": {
    "name": "CanalEstacion"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nombre": {
      "type": "string"
    },
